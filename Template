<CSVLink 
  data={[
    ...cpuData.map(item => ({
      datetime: item.datetime,
      ...Object.fromEntries(
        Object.entries(item).filter(([key]) => key.startsWith('Instance'))
      ),
      metric: 'CPU'
    })),
    ...diskData.map(item => ({
      datetime: item.datetime,
      ...Object.fromEntries(
        Object.entries(item).filter(([key]) => key.startsWith('Instance'))
      ),
      metric: 'Disk'
    })),
    ...memoryData.map(item => ({
      datetime: item.datetime,
      ...Object.fromEntries(
        Object.entries(item).filter(([key]) => key.startsWith('Instance'))
      ),
      metric: 'Memory'
    }))
  ]} 
  filename={"utilization_data.csv"}
>
  <button className="graph-btn graph-secondary">Export CSV</button>
</CSVLink>
